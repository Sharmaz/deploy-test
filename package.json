{
  "name": "deploy-test",
  "version": "0.1.0",
  "description": "Test github actions deploy to server instance.",
  "main": "index.js",
  "scripts": {
    "dev": "node index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:revert": "sequelize-cli db:migrate:undo",
    "migrations:delete": "sequelize-cli db:migrate:undo:all",
    "migrations:seed:generate": "sequelize-cli seed:generate --name",
    "migrations:run:seed": "sequelize-cli db:seed:all",
    "migrations:seed:revert": "sequelize-cli db:seed:undo",
    "migrations:seed:delete": "sequelize-cli db:seed:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sharmaz/deploy-test.git"
  },
  "author": "Ivan Robles <irae45@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Sharmaz/deploy-test/issues"
  },
  "homepage": "https://github.com/Sharmaz/deploy-test#readme",
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mysql2": "^3.6.0",
    "sequelize": "^6.32.1"
  },
  "devDependencies": {
    "sequelize-cli": "^6.6.1"
  }
}
